{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "ubuntu-interface",
    "labels": {
      "lab": "example",
      "system": "client",
      "net": "internal"
    }
  },
  "spec": {
    "selector": {
      "lab": "example",
      "system": "client",
      "net": "internal"
    },
    "ports": [
      {
        "name": "http",
        "port": 80,
        "targetPort": 80
      },
      {
        "name": "tty",
        "port": 8080,
        "targetPort": 8080
      },
      {
        "name": "vnc",
        "port": 5900,
        "targetPort": 5900
      }
    ]
  }
}